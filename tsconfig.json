{
    "compilerOptions": {
        "esModuleInterop": true,
        "target": "es5",
        "lib": ["dom", "es2015"],
        "module": "commonjs",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "strict": true,
        "types": ["cypress", "node"],
        "jsx": "react",
        "sourceMap": true,
        "outDir": "./dist",
        "resolveJsonModule": true,
        "allowJs": true,
        "noEmit": true
    },
    "include": ["**/*.*"],
    "exclude": ["node_modules"]
}
